		<!-- Maven Plugins -->
		<pluginManagement>
			<plugins>
				<plugin> <!-- Set a compiler level -->
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>${plugin.apache.compiler.version}</version>
					<configuration>
						<source>${jdk.version}</source>
						<target>${jdk.version}</target>
					</configuration>
				</plugin>
				<plugin> <!-- exec:java -->
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>exec-maven-plugin</artifactId>
					<version>${plugin.codehaus.exec.version}</version>
					<configuration>
					</configuration>
				</plugin>

				<!-- Enforcer -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-enforcer-plugin</artifactId>
					<version>${plugin.apache.enforcer.version}</version>
					<executions>
						<execution>
							<id>enforcer-dependency-checks</id>
							<goals>
								<goal>enforce</goal>
							</goals>
							<configuration>
								<rules>
									<dependencyConvergence />
									<bannedDependencies>
										<excludes>
											<!-- Logging Mechanism : Refer https://www.slf4j.org/legacy.html -->
											<!-- Using sl4j logger bound to logback-classic -->
											<!-- Required binders: log4j-over-slf4j | jcl-over-slf4j | jul-to-slf4j 
												| logback-core | logback-classic | slf4j-api -->
											<exclude>org.slf4j:slf4j-log4j12</exclude> <!-- Don't use with log4j-over-slf4j -->
											<exclude>org.slf4j:slf4j-jcl</exclude> <!-- Don't use with jcl-over-slf4j -->
											<exclude>org.slf4j:slf4j-jdk14</exclude> <!-- Don't use with jul-to-slf4j -->
										</excludes>
									</bannedDependencies>
								</rules>
								<fail>true</fail>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin> <!-- Run Ant Ivy Task to install in .ivy repo ::NOT USED -->
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-antrun-plugin</artifactId>
					<version>${plugin.apache.antrun.version}</version>
					<executions>
						<execution>
							<phase>install</phase>
							<configuration>
								<target>
									<taskdef name="ivy-publish"
										classname="org.apache.ivy.ant.IvyPublish"
										classpathref="maven.plugin.classpath" />
									<ivy-publish resolver="local" overwrite="true"
										organisation="${project.groupId}"
										module="${project.artifactId}" revision="${project.version}">
										<artifacts
											pattern="${project.build.directory}\${project.artifactId}-${project.version}.jar" />
									</ivy-publish>
								</target>
							</configuration>
							<goals>
								<goal>run</goal>
							</goals>
						</execution>
					</executions>
					<dependencies>
						<dependency>
							<groupId>org.apache.ant</groupId>
							<artifactId>ant</artifactId>
							<version>1.10.6</version>
						</dependency>
						<dependency>
							<groupId>org.apache.ivy</groupId>
							<artifactId>ivy</artifactId>
							<version>2.4.0</version>
						</dependency>
					</dependencies>
				</plugin>
				<plugin> <!-- UIMA | generate types dynamically from type-system descriptor xml files -->
					<groupId>org.apache.uima</groupId>
					<artifactId>jcasgen-maven-plugin</artifactId>
					<version>${uimaj.version}</version>
					<executions>
						<execution>
							<phase>generate-sources</phase>
							<goals>
								<goal>generate</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
				<plugin> <!-- UIMA | consume generated-sources/jcasgen -->
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>build-helper-maven-plugin</artifactId>
					<version>${plugin.codehaus.buildhelper.version}</version>
					<executions>
						<execution>
							<id>addToSourceFolder</id>
							<goals>
								<!--add the generated sources -->
								<goal>add-source</goal>
							</goals>
							<phase>process-sources</phase>
						</execution>
					</executions>
				</plugin>


				<!-- Code formatter -->
				<plugin>
					<groupId>com.diffplug.spotless</groupId>
					<artifactId>spotless-maven-plugin</artifactId>
					<version>${plugin.diffplug.spotless.version}</version>
					<configuration>
						<java>
							<googleJavaFormat>
								<style>GOOGLE</style>
							</googleJavaFormat>
							<removeUnusedImports />
							<importOrder>
								<order>scala,java,javax,org,com,uk</order>
							</importOrder>
						</java>
					</configuration>
					<executions>
						<execution>
							<phase>compile</phase>
							<goals>
								<goal>apply</goal>
							</goals>
						</execution>
					</executions>
