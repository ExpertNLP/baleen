								<goal>verify</goal>
							</goals>
							<configuration>
								<skipITs>${skip.integration.tests}</skipITs>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<!-- Javadocs plugin -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-javadoc-plugin</artifactId>
					<version>${plugin.apache.javadoc.version}</version>
					<executions>
						<execution>
							<id>attach-javadocs</id>
							<goals>
								<goal>jar</goal>
							</goals>
							<configuration>
								<skip>${skip.javadoc}</skip>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-assembly-plugin</artifactId>
					<version>${plugin.apache.assembly.version}</version>
				</plugin>

				<!-- Plugins to be used ONLY at top-project level -->

				<plugin> <!-- download/collect license files from project dependencies -->
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>license-maven-plugin</artifactId>
					<version>${plugin.codehause.license.version}</version>
					<configuration>
						<outputDirectory>${env.DEV_HOME}/baleen/reports</outputDirectory> <!-- THIRDPARTY.txt -->
						<licensesOutputDirectory>${env.DEV_HOME}/baleen/reports/licenses</licensesOutputDirectory>
						<cleanLicensesOutputDirectory>false</cleanLicensesOutputDirectory>
						<useDefaultContentSanitizers>true</useDefaultContentSanitizers>
						<organizeLicensesByDependencies>true</organizeLicensesByDependencies>
						<useDefaultUrlReplacements>true</useDefaultUrlReplacements>
						<projectName>baleen</projectName>
						<skipAggregateAddThirdParty>${skip.thirdparty.report}</skipAggregateAddThirdParty>
						<skipAggregateThirdPartyReport>${skip.thirdparty.report}</skipAggregateThirdPartyReport>
						<skipUpdateProjectLicense>${skip.licenses.report}</skipUpdateProjectLicense>
						<skipAggregateDownloadLicenses>${skip.licenses.report}</skipAggregateDownloadLicenses>
					</configuration>
					<executions>
						<execution>
							<id>third-party-licenses</id>
							<phase>generate-resources</phase>
							<goals>
								<goal>aggregate-add-third-party</goal>
								<goal>aggregate-third-party-report</goal>
								<goal>aggregate-download-licenses</goal>
							</goals>
						</execution>
					</executions>
				</plugin>

				<!-- Version Report -->
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>versions-maven-plugin</artifactId>
					<version>${plugin.codehaus.versions.version}</version>
					<executions>
						<execution>
							<id>dependency-version-report</id>
							<goals>
								<goal>dependency-updates-report</goal>
								<goal>plugin-updates-report</goal>
								<goal>property-updates-report</goal>
							</goals>
							<phase>generate-resources</phase>
						</execution>
					</executions>
					<configuration>
						<outputDirectory>${env.DEV_HOME}/baleen/reports</outputDirectory>
						<skip>${skip.version.report}</skip>
					</configuration>
				</plugin>
				<!-- Dependency Tree Report -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-dependency-plugin</artifactId>
					<version>${plugin.apache.dependency.version}</version>
					<executions>
						<execution>
							<id>dependency-tree-reports</id>
							<phase>generate-resources</phase>
							<goals>
								<goal>tree</goal>
							</goals>
						</execution>
					</executions>
					<configuration>
						<appendOutput>false</appendOutput>
						<outputType>dot</outputType>
						<outputFile>${project.build.directory}/dependency-tree.dot</outputFile>
						<skip>${skip.dependencytree.report}</skip>
					</configuration>
				</plugin>

			</plugins>
		</pluginManagement>
	</build>
</project>
